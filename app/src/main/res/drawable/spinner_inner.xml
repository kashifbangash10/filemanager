<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="0"
    android:toDegrees="720"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape
        android:shape="ring"
        android:innerRadiusRatio="4.5"
        android:thicknessRatio="40"
        android:useLevel="false">
        <gradient
            android:type="sweep"
            android:startColor="#00BBDEFB"
            android:centerColor="#4DBBDEFB"
            android:endColor="#BBDEFB" />
    </shape>
</rotate>
