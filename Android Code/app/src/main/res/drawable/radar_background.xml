<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer Circle -->
    <item>
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="#40FFFFFF" />
        </shape>
    </item>
    <!-- Middle Circle -->
    <item android:top="30dp" android:bottom="30dp" android:left="30dp" android:right="30dp">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="#30FFFFFF" />
        </shape>
    </item>
    <!-- Inner Circle -->
    <item android:top="60dp" android:bottom="60dp" android:left="60dp" android:right="60dp">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="#20FFFFFF" />
        </shape>
    </item>
</layer-list>
